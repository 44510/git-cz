{
  "name": "git-cz",
  "version": "0.1.0",
  "description": "Semantic emojified git commit",
  "main": "lib/index.js",
  "bin": {
    "ci": "./bin/git-cz.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/streamich/git-cz.git"
  },
  "homepage": "https://github.com/streamich/git-cz",
  "license": "Unlicense",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "inquirer": "^3.1.1",
    "word-wrap": "^1.2.3",
    "pad-right": "^0.2.2",
    "signale": "^1.1.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "commitizen": "^2.9.6",
    "eslint": "^4.19.1",
    "eslint-config-mailonline": "^9.0.0",
    "husky": "^0.14.3"
  },
  "config": {
    "commitizen": {
      "path": "./index.js",
      "changelog": {
        "maxMessageLength": 72,
        "list": [
          "feat",
          "fix",
          "chore",
          "docs",
          "refactor",
          "style",
          "ci"
        ]
      }
    }
  }
}
